      "source": [
        "import dask.dataframe as ddf # 데이터를 늘 메모리에 올릴 필요가 없을 때\n",
        "\n",
        "ddf = ddf.read_csv('/example.csv',\n",
        "                   dtype = {:, :}) # 데이터 프레임에 대한 meta 정보 생성\n",
        "\n",
        "ddf.head(10) # -> 필요한 데이터를 가져옴\n",
        "\n",
        "ddf['column1'].sum().compute() # compute를 걸어야 데이터를 가져옴"
      ]
