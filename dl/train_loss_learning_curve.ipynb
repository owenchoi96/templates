        "# -- learning curve --\n",
        "\n",
        "def calc_moving_average(arr, win_size=100):\n",
        "    new_arr = []\n",
        "    win = []\n",
        "\n",
        "    for i, val in enumerate(arr):\n",
        "        win.append(val)\n",
        "        if len(win) > win_size:\n",
        "            win.pop(0)\n",
        "\n",
        "        new_arr.append(np.mean(win))\n",
        "    return np.array(new_arr)"
      ]
    }
  ]
